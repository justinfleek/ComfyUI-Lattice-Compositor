{
  "name": "@image-metahub/metadata-engine",
  "version": "1.0.0-beta.1",
  "description": "Parse AI-generated image metadata from 15+ formats including ComfyUI, InvokeAI, Automatic1111, DALL-E, and more",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "metadata",
    "image",
    "ai",
    "stable-diffusion",
    "comfyui",
    "invokeai",
    "automatic1111",
    "midjourney",
    "dalle",
    "exif",
    "png",
    "jpeg"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LuqP2/image-metahub.git",
    "directory": "packages/metadata-engine"
  },
  "author": "LuqP2",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "exifr": "^7.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.2.2",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0"
  }
}
